@model IEnumerable<TeamMember>

<table id="collectionTable" class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach(TeamMember entity in this.Model)
        {
            <tr>
                <td>@entity.TeamMemberId</td>
                <td>@entity.FirstName</td>
                <td>@entity.LastName</td>
                <td>@entity.Email</td>
            </tr>
        }
    </tbody>
</table>